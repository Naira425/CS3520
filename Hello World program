#Hello world c++ code with corresponding file
##include <iostream>
#using namespace std;
#
#int main() {
#  
#   cout << "Hello World!" << endl;
#
#   return 0;
#}


.data 
msg: 
    .asciz "Hello World!\n"   # Null-terminated string

.text
.globl _start
_start:
    la a0, msg               # Load address of string into a0
    li a7, 4                 
    ecall                    

    li a7, 10                # System call: exit
    ecall
